        setclipboard("https://www.youtube.com/@exe_evil")
    end)

    Discord.MouseButton1Click:Connect(function() 
        setclipboard("https://discord.gg/p4nbCfs7zW")
    end)

    SocialMedia.MouseButton1Click:Connect(function() 
        setclipboard("Copy SOCIAL MEDIA LINK")
    end)
    
        Submit.MouseButton1Click:Connect(function()
        local inputKey = KeyInput.Text
        if inputKey == "" then
            return
        end

        if verifyKey(inputKey) then
            KeySystem.SaveKey(inputKey)
            keyAuthenticated = true
            wait(1)
            ScreenGui:Destroy()
            StartAutoBounty()
        end
    end)

    local startTime = tick()
    local timeout = 300
    repeat wait(0.1) until keyAuthenticated or (tick() - startTime > timeout)

    if keyAuthenticated then
        return true
    else
        if ScreenGui and ScreenGui.Parent then
            ScreenGui:Destroy()
        end
        return false
    end
end

local keyValid = KeySystem.CreateKeyUI()
if not keyValid then
    warn("Key is invalid or timed out. Script stopped.")
    return
end

